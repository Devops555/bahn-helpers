{
  "name": "tiny-helpers",
  "routes": [
    {
      "src": "/",
      "dest": "/home/index.html"
    },
    {
      "src": "/?sortBy=date",
      "dest": "/home-sorted-by-date/index.html"
    },
    {
      "src": "/api/?sortBy=date",
      "dest": "/css-sorted-by-date/index.html"
    }
  ],
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  }
}
